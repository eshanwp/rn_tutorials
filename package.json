{
  "name": "rn_tutorials",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:android": "cd android/app && rm -rf build",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "clean:ios": "npm run clean:pods && cd ios && rm -rf build",
    "clean:modules": "rm -rf node_modules && yarn",
    "install:pods": "cd ios && pod install",
    "remove:ios:build": "cd ios && rm -rf build",
    "ios:13": "react-native run-ios --simulator=\"iPhone 13\"",
    "storybook-generate": "sb-rn-get-stories"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-config": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-community/slider": "^4.5.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@storybook/addon-ondevice-actions": "^7.6.20",
    "@storybook/addon-ondevice-controls": "^7.6.20",
    "@storybook/react-native": "^7.6.20",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-dom": "18.2.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
